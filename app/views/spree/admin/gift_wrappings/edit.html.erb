<% content_for :page_actions do %>
  <li><%= button_link_to t(:back_to_gift_wrappings_list), admin_gift_wrappings_url, :icon => 'icon-arrow-left' %></li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @gift_wrapping } %>

<%= form_for [:admin, @gift_wrapping], :method => :put, :html => { :multipart => true } do |f| %>
  <fieldset class="no-border-top">
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>